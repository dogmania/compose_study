<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.MyApplication"
        tools:targetApi="31">
        <activity
            android:name=".ViewModelDemoActivity"
            android:exported="true"
            android:label="@string/title_activity_view_model_demo"
            android:theme="@style/Theme.MyApplication">

            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>

        </activity>
        <activity
            android:name=".IntroductionViewModelActivity"
            android:exported="true"
            android:label="@string/title_activity_introduction_view_model"
            android:theme="@style/Theme.MyApplication">

        </activity>
        <activity
            android:name=".LazyListDemo"
            android:exported="true"
            android:label="@string/title_activity_lazy_list_demo"
            android:theme="@style/Theme.MyApplication"></activity>
        <activity
            android:name=".ListDemoActivity"
            android:exported="false"
            android:label="@string/title_activity_list_demo"
            android:theme="@style/Theme.MyApplication" />
        <activity
            android:name=".LazyListActivity"
            android:exported="true"
            android:label="@string/title_activity_lazy_list"
            android:theme="@style/Theme.MyApplication" />
        <activity
            android:name=".CoroutineActivity"
            android:exported="true"
            android:label="@string/title_activity_coroutine"
            android:theme="@style/Theme.MyApplication" />
        <activity
            android:name=".IntrinsicSizeDemoActivity"
            android:exported="true"
            android:label="@string/title_activity_intrinsic_size_demo"
            android:theme="@style/Theme.MyApplication" />
        <activity
            android:name=".MainActivity"
            android:exported="true"
            android:label="@string/app_name"
            android:theme="@style/Theme.MyApplication" />
    </application>

</manifest>